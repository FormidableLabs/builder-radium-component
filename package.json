{
  "name": "builder-radium-component",
  "version": "0.2.0",
  "description": "Builder Archetype - Radium Component",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/builder-radium-component.git"
  },
  "author": "Ryan Roemer <ryan.roemer@formidablelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/builder-radium-component/issues"
  },
  "homepage": "https://github.com/FormidableLabs/builder-radium-component",
  "scripts": {
    "npm:postinstall": "cd lib || builder run build-lib",
    "npm:preversion": "builder run check",
    "npm:version": "builder run clean && builder run build && git add -A dist",
    "npm:test": "builder run test-frontend",
    "clean-dist": "rimraf dist",
    "build-dist-min": "webpack --bail --config node_modules/builder-radium-component/config/webpack/webpack.config.babel.js",
    "build-dist-dev": "webpack --bail --config node_modules/builder-radium-component/config/webpack/webpack.config.dev.babel.js",
    "build-dist": "builder run clean-dist && builder run build-dist-min && builder run build-dist-dev",
    "clean-lib": "rimraf lib",
    "build-lib": "builder run clean-lib && babel src -d lib --copy-files",
    "clean": "builder run clean-lib && builder run clean-dist",
    "build": "builder run build-lib && builder run build-dist",
    "server-dev": "webpack-dev-server --port 3000 --config node_modules/builder-radium-component/config/webpack/demo/webpack.config.dev.babel.js --content-base demo",
    "server-hot": "webpack-dev-server --port 3000 --config node_modules/builder-radium-component/config/webpack/demo/webpack.config.hot.babel.js --hot --content-base demo",
    "server-test": "webpack-dev-server --port 3001 --config node_modules/builder-radium-component/config/webpack/webpack.config.test.babel.js",
    "dev": "builder concurrent server-dev server-test",
    "hot": "builder concurrent server-hot server-test",
    "open-demo": "opener http://127.0.0.1:3000",
    "open-dev": "builder concurrent dev open-demo",
    "open-hot": "builder concurrent hot open-demo",
    "lint-server": "eslint --color -c node_modules/builder-radium-component/config/eslint/.eslintrc-server *.js",
    "lint-client": "eslint --color --ext .js,.jsx -c node_modules/builder-radium-component/config/eslint/.eslintrc-client src demo/*.jsx",
    "lint-client-test": "eslint --color --ext .js,.jsx -c node_modules/builder-radium-component/config/eslint/.eslintrc-client-test src test/client",
    "lint": "builder concurrent lint-server lint-client lint-client-test",
    "test-frontend": "karma start node_modules/builder-radium-component/config/karma/karma.conf.js",
    "test-frontend-ci": "karma start --browsers PhantomJS,Firefox node_modules/builder-radium-component/config/karma/karma.conf.coverage.js",
    "test-frontend-cov": "karma start node_modules/builder-radium-component/config/karma/karma.conf.coverage.js",
    "test-frontend-dev": "karma start node_modules/builder-radium-component/config/karma/karma.conf.dev.js",
    "test": "builder run npm:test",
    "test-ci": "builder run test-frontend-ci",
    "test-cov": "builder run test-frontend-cov",
    "test-dev": "builder run test-frontend-dev",
    "check": "builder run lint && builder run test",
    "check-ci": "builder run lint && builder run test-ci",
    "check-cov": "builder run lint && builder run test-cov",
    "check-dev": "builder run lint && builder run test-dev",
    "builder:lint-server": "eslint --color -c config/eslint/.eslintrc-server *.js config/karma config/webpack",
    "builder:lint": "npm run builder:lint-server",
    "builder:check": "npm run builder:lint"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-syntax-class-properties": "^6.3.13",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-webpack": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "^2.0.4-beta"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "builder-support": "0.2.0",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^8.0.2",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-flow-vars": "^0.1.3",
    "eslint-plugin-react": "^3.15.0",
    "lodash.clonedeep": "^4.0.1",
    "lodash.merge": "^4.0.1",
    "lodash.omit": "^4.0.1"
  }
}
